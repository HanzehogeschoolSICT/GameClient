<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane id="symbol" xmlns:fx="http://javafx.com/fxml/1" >
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="110.0" prefWidth="110.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="110.0" prefWidth="110.0" />
    </columnConstraints>
     <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <ListView fx:id="toChallenge" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
        <ListView fx:id="challenges" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
        <Button onAction="#challengePlayer" text="Challenge" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
        <Button onAction="#acceptChallenge" text="Accept" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
    </children>
</GridPane>
