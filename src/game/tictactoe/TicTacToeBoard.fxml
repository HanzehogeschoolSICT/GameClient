<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane id="tictactoeBoard" 
            xmlns:fx="http://javafx.com/fxml" 
            fx:id="grid" 
            gridLinesVisible="false" 
            maxHeight="600.0" 
            maxWidth="600.0"
            minHeight="600.0" 
            minWidth="600.0"
            prefHeight="600.0" 
            prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.121">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="205.0" maxWidth="205.0" prefWidth="200.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="205.0" maxWidth="205.0" prefWidth="200.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="205.0" maxWidth="205.0" prefWidth="200.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="200.0" maxHeight="205.0" prefHeight="205.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="200.0" maxHeight="205.0" prefHeight="205.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="200.0" maxHeight="205.0" prefHeight="205.0" vgrow="SOMETIMES" />
  </rowConstraints>
    <children>
      <Label id="tictactoeFieldTopLeft"     prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="0" GridPane.rowIndex="0" />
      <Label id="tictactoeFieldTop"         prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      <Label id="tictactoeFieldTopRight"    prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="2" GridPane.rowIndex="0" />
      <Label id="tictactoeFieldLeft"        prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="0" GridPane.rowIndex="1" />
      <Label id="tictactoeField"            prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label id="tictactoeFieldRight"       prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <Label id="tictactoeFieldBottomLeft"  prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="0" GridPane.rowIndex="2" />
      <Label id="tictactoeFieldBottom"      prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label id="tictactoeFieldBottomRight" prefHeight="205.0" prefWidth="205.0" onMouseClicked="#squareClicked" GridPane.columnIndex="2" GridPane.rowIndex="2" />
    </children>
</GridPane>


